{
  "name": "DrDocx",
  "version": "0.1.4-alpha",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "@date-io/moment": "1.x",
    "@material-ui/core": "^4.9.12",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/pickers": "^3.2.10",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^0.19.2",
    "cross-env": "^7.0.2",
    "electron-is-dev": "^1.2.0",
    "electron-updater": "^4.3.1",
    "immutability-helper": "^3.0.2",
    "material-table": "^1.61.0",
    "moment": "^2.25.3",
    "notistack": "^0.9.11",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "react-router-transition": "^2.0.0",
    "react-scripts": "3.4.1"
  },
  "scripts": {
    "start": "react-scripts start --no-cache",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "pack": "NOTARIZE=0 react-scripts build && electron-builder --publish never",
    "dist": "NOTARIZE=1 react-scripts build && electron-builder --publish never",
    "release": "react-scripts build && electron-builder --publish always"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "no-unused-vars": "off"
    }
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "dotenv": "^8.2.0",
    "electron": "^8.2.5",
    "electron-builder": "^22.6.0",
    "electron-notarize": "^1.0.0",
    "wait-on": "^4.0.2"
  },
  "build": {
    "extends": null,
    "productName": "DrDocx",
    "appId": "com.faizsurani.drdocx",
    "afterSign": "osx/notarize.js",
    "publish": [{
      "provider": "github",
      "owner": "DrDocx",
      "repo": "DrDocx-Desktop"
    }],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "hardenedRuntime": true,
      "entitlements": "./osx/entitlements.mac.inherit.plist"
    },
    "directories": {
      "buildResources": "assets"
    },
    "files": [
      "public/",
      "build/"
    ],
    "extraFiles": [
      "api-bin"
    ],
    "nsis": {
      "oneClick": false,
      "installerIcon": "icon.ico"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version"
    ]
  }
}
